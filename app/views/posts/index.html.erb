<br />
<h2>교수진</h2>

<!-- 론 위즐리 -->

<div class="ui link cards">
  <div class="card">
    <div class="ui small fade reveal image">
      <div class="hidden content" >
        <img src="data:image/jpeg;base64," alt="해리포터">
      </div>
      <div class="visible content" >
        <img src="https://www.trendsinternational.com/media/catalog/product/cache/1/image/9df78eab33525d08d6e5fb8d27136e95/1/6/16721-harry-potter---gryffindoor-illustrated---4x6.jpg ">
      </div>
    </div>
    <div class="content">
      <div class="header">Matt Giampietro</div>
      <div class="meta">
        <a>Friends</a>
      </div>
      <div class="description">
        Matthew is an interior designer living in New York.
      </div>
    </div>
    <div class="extra content">
      <span class="right floated">
        Joined in 2013
      </span>
      <span>
        <i class="user icon"></i>
        75 Friends
      </span>
    </div>
  </div>
</div>



<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th>Title</th>
        <th>Data actions</th>
      </tr>
    </thead>

    <tbody>
      <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td>
          <%
=begin%>
          댓글
          <%
=end%>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>

  <%= link_to 'New Post', new_domitory_post_path, class: 'btn btn-default' %>
</div>